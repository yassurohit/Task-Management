{
  "version": 2,
  "builds": [
    { "src": "manage.py", "use": "vercel-python@3.1.7" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    { "src": "/(.*)", "dest": "manage.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "tasks.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
